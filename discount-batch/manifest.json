{
  "manifest_version": 2,
  "name": "优惠分批",
  "version": "1.0",
  "description": "优惠分批",
  "author": "libaojie",
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "js/jquery-1.8.3.js",
        "js/content-script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "inject.js"
  ]
}